@Model 
@{
    ViewBag.Title = "Signup";
}


<title>MY FIRST WEBSITE </title>

<h1><strong>Register</strong></h1>



<div id="FormDiv">
    <form method="post" action="/Signup/Profile">
        <table id="table">
            <tr><th colspan="2" id="headerSignup"></th></tr>
            <tr>
                <td>User Name:</td>
                
                <td><input id="uName" type="text" name="Usrname" placeholder="User Name" /></td>

            </tr>
            <tr>
                <td>Email:</td>
                <td><input id="text" type="text" name="email" placeholder="Email" /></td>

            </tr>
         <tr>
                <td>Password</td>
                <td><input id="pwd" type="password" placeholder="Password" name="cpwd" /></td>

            </tr>
            <tr>
                <td>Confirm Password:</td>
                <td><input id="cpwd" type="password" placeholder="Confirm Your Password" name="cpwd" /></td>

            </tr>
            <tr>
                <td>Address: </td>
                <td><input id="addr" type="text" placeholder=" Enter your Address" name="Addr"/></td>

            </tr>
            <tr>
                <td>Pincode</td>
                <td><input id="pin" type="number" placeholder="Pincode" name="pinCode" /></td>

            </tr>
            <tr>
                <td>State</td>
                <td><input id="state" type="text" placeholder="State" name="State" /></td>

            </tr>


        </table>
        <button id="button1" type="submit"  class="btn btn-default" value="Submit" >Submit</button>
    </form>
</div>




